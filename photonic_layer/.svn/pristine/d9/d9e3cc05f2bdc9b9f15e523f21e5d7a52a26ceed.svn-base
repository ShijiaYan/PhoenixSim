package PhotonicElements.InputSources.LaserEfficiency;

public abstract class AbstractLaserEfficiency {

	// need to have output power and WPE as a function of drive current and temperature
	
	public abstract double getEfficiencyFromCurrent(double currentmA, double tempCelsius) ;
	public abstract double getOpticalPowermWfromCurrent(double currentmA, double tempCelsius) ;
	public abstract double getEFficiencyFromOpticalPowermW(double powermW, double tempCelsius) ;
	public abstract double getEfficiencyFromOpticalPowerdBm(double powerdBm, double tempCelsius) ;
	public abstract double getCurrentmAfromOpticalPowermW(double powermW, double temCelsius) ;
	
	
}
