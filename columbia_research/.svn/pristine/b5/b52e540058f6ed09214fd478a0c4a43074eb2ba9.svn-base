package edu.columbia.ke.DataStructure;

public class CircuitStateInfo extends SrcDstStackDistanceRecord{
	
	private double inCacheSince;
	public double getInCacheSince() {
		return inCacheSince;
	}

	public void setInCacheSince(double inCacheSince) {
		this.inCacheSince = inCacheSince;
	}

	private int src;
	private int dst;

	public CircuitStateInfo(int src, int dst) {
		super();
		this.src = src;
		this.dst = dst;
		this.inCacheSince = -1;
	}
}
